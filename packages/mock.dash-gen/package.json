{
  "name": "mock-dash-gen",
  "type": "module",
  "version": "0.0.1",
  "description": "A code generator for mock-dash based on Zod schemas.",
  "author": "Eskil Ganslandt",
  "license": "MIT",
  "homepage": "https://github.com/MrPorky/mock-dash",
  "bugs": "https://github.com/MrPorky/mock-dash/issues",
  "keywords": [
    "typescript",
    "zod",
    "api",
    "client",
    "mock",
    "server",
    "type-safe",
    "schema",
    "generation",
    "codegen"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist node_modules",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "openapi3-ts": "^4.5.0",
    "tsdown": "^0.15.7",
    "vitest": "^3.2.4"
  }
}
