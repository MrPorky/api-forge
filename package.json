{
  "name": "mock-dash",
  "type": "module",
  "version": "0.0.4",
  "description": "A TypeScript library for generating type-safe API clients and mock servers from Zod schemas",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/MrPorky/mock-dash",
  "bugs": "https://github.com/MrPorky/mock-dash/issues",
  "keywords": [
    "typescript",
    "zod",
    "api",
    "client",
    "mock",
    "server",
    "type-safe",
    "schema",
    "validation",
    "hono"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.3",
    "hono": "^4.9.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@faker-js/faker": "^10.0.0",
    "@types/node": "^24.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-format": "^1.0.2",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "zocker": "^3.0.0",
    "zod": "^4.1.11",
    "zod-schema-faker": "2.0.0-beta.6"
  }
}
